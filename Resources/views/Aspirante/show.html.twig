{% extends 'ItsurAeiBundle::layout.html.twig' %}
{% block title %}
    Mostrar Aspirante
{% endblock %}

{% block body %}
    Ficha: {{aspirante.ficha}}
    <br/>
    Nombre: {{aspirante.nombre}}
    <br/>
    Carrera: {{aspirante.carrera}}
    <br/>
    Lugar de Origen: {{aspirante.lugarOrigen}}
    <br/>
    Escuela de Procedencia: {{aspirante.escuelaProcedencia}}
    <br/>
    Bachillerato: {{aspirante.bachillerato}}
    <br/>
    Genero: {{aspirante.genero}}
    <br/>
    Fecha de Nacimiento: {{aspirante.fechaNacimiento.format('d-m-Y')}}
    <br/>
    Promedio Bachillerato: {{aspirante.promedioBachillerato}}

{% endblock %}
