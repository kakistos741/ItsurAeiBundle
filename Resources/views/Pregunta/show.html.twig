<h1>Pregunta</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Sentencia</th>
            <td>{{ entity.sentencia }}</td>
        </tr>
        <tr>
            <th>Objetivo</th>
            <td>{{ entity.objetivo }}</td>
        </tr>
        <tr>
            <th>Imagen</th>
            <td>{{ entity.imagen }}</td>
        </tr>
        <tr>
            <th>Respuestaimagenes</th>
            <td>{{ entity.respuestaImagenes }}</td>
        </tr>
        <tr>
            <th>Opcion1</th>
            <td>{{ entity.opcion1 }}</td>
        </tr>
        <tr>
            <th>Opcion2</th>
            <td>{{ entity.opcion2 }}</td>
        </tr>
        <tr>
            <th>Opcion3</th>
            <td>{{ entity.opcion3 }}</td>
        </tr>
        <tr>
            <th>Opcion4</th>
            <td>{{ entity.opcion4 }}</td>
        </tr>
        <tr>
            <th>Respuesta</th>
            <td>{{ entity.respuesta }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('pregunta') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('pregunta_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('pregunta_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
